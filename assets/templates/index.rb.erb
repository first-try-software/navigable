# frozen-string-literal: true

require_relative '../application_action'
require_relative './<%= @entity_name %>'
require_relative './repository'

module <%= @resource_klass %>
  class Index < ApplicationAction
    def execute
      successfully found_<%= @resource_name %>
    end

    private

    def found_<%= @resource_name %>
      @found_<%= @resource_name %> ||= Repository.all
    end
  end
end
